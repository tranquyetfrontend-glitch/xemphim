{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/Frontend/Feature/home/index.html"
    },
    {
      "source": "/home",
      "destination": "/Frontend/Feature/home/index.html"
    },
    {
      "source": "/home/(.*)",
      "destination": "/Frontend/Feature/home/$1"
    },
    {
      "source": "/auth/(.*)",
      "destination": "/Frontend/Feature/auth/$1"
    },
    {
      "source": "/detail/(.*)",
      "destination": "/Frontend/Feature/detail/$1"
    },
    {
      "source": "/payment/(.*)",
      "destination": "/Frontend/Feature/payment/$1"
    },
    {
      "source": "/user/(.*)",
      "destination": "/Frontend/Feature/user/$1"
    },
    {
      "source": "/common/(.*)",
      "destination": "/Frontend/Feature/common/$1"
    },
    {
      "source": "/Share/(.*)",
      "destination": "/Frontend/Share/$1"
    }
  ]
}